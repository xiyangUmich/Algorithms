class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int n=nums.size();
        if(n>=1){
        sort(nums.begin(),nums.end());
        int i=0;
        while(nums[i]<val&&i<n) i++;
        if(i!=n){
        while(nums[i]==val&&i<n){
            nums.erase(nums.begin()+i);
            n--;
            
        }
        }
        }
        return n;
    }
};
