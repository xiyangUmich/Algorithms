class Solution {
public:
    int majorityElement(vector<int>& nums) {
       sort(nums.begin(),nums.end());
       int n=nums.size();
       if(n==1) return nums[0];
       int i=0;
       int count=1;
       while(i<n){
           if(nums[i]==nums[i+1]){
               count++;
               i++;
           }
           else{
               count=1;
               i++;
           }
           if(count>n/2)
                return nums[i-1];
       }
    }
};
